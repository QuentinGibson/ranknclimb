{
  "name": "rank-n-climb",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "author": "Quentin Gibson",
  "scripts": {
    "dev": "concurrently \"npm:next:dev\" \"npm:slicemachine\" --names \"next,slicemachine\" --prefix-colors blue,magenta",
    "dev:inspect": "concurrently \"npm:next:dev:inspect\" \"npm:slicemachine\" --names \"next-inspect,slicemachine\" --prefix-colors blue,magenta",
    "dev:turbo": "concurrently \"npm:next:dev:turbo\" \"npm:slicemachine\" --names \"next-turbo,slicemachine\" --prefix-colors blue,magenta",
    "next:dev:turbo": "next dev --turbo",
    "next:dev": "next dev",
    "next:dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "slicemachine": "start-slicemachine",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.9.9",
    "@prisma/client": "^6.2.1",
    "@prismicio/client": "^7.14.0",
    "@prismicio/next": "^1.7.1",
    "@prismicio/react": "^2.9.1",
    "@sentry/nextjs": "^8",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.3",
    "clsx": "^2.1.1",
    "lucide-react": "^0.373.0",
    "next": "^15.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@slicemachine/adapter-next": "^0.3.38",
    "@storybook/addon-essentials": "^8.1.1",
    "@storybook/addon-interactions": "^8.1.1",
    "@storybook/addon-links": "^8.1.1",
    "@storybook/addon-onboarding": "^8.1.1",
    "@storybook/blocks": "^8.1.1",
    "@storybook/nextjs": "^8.1.1",
    "@storybook/react": "^8.1.1",
    "@storybook/test": "^8.1.1",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "slice-machine-ui": "^1.26.0",
    "storybook": "^8.1.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  }
}
